{
	"env": {
		"node": true,
		"es2020": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["import"],
	"rules": {
		"no-unused-vars": "warn",
		"@typescript-eslint/no-unused-vars": "warn",
		"import/no-unresolved": "error"
	}
}
